int a(int x)
{
    if (x == 0)
    {
        print("x is 0");
    }
    else if (x == 1)
    {
        print("x is 1");
    }
    else if (x == 2)
    {
        print("x is 2");
    }
    else
    {
        print("x is something else");
    }
}

int main()
{
    a(0);
    a(1);
    a(2);
    a(3);
}
