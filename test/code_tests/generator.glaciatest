generator numbers(int total)
{
    int i = 1;

    while (i <= total)
    {
        yield i;
        i = i + 1;
    }
}

int main()
{
    generator gen = numbers(3);

    int i = 0;
    while (i < 4)
    {
        print(gen.next());
        print(gen.finished());

        i = i + 1;
    }
}
